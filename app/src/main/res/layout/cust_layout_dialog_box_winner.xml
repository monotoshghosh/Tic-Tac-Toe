<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="320dp"
    android:layout_height="290dp"
    android:layout_gravity="center_horizontal"
    android:background="@color/white">


    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="175dp"
        app:layout_constraintBottom_toTopOf="@+id/cancel_dialog_winner"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/celeID1"
            android:layout_width="55dp"
            android:layout_height="wrap_content"
            android:layout_marginRight="6dp"
            android:src="@drawable/cele1" />

        <TextView
            android:id="@+id/winnerName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:text="Monotosh"
            android:textColor="#49454F"
            android:textSize="35dp"
            android:textStyle="bold" />

        <ImageView
            android:id="@+id/celeID2"
            android:layout_width="55dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="6dp"
            android:src="@drawable/cele2" />
    </LinearLayout>

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:text="Win"
        android:textColor="#F4AF04"
        android:textSize="35dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/linearLayout2" />

    <ImageView
        android:id="@+id/winnerTrophyImg"
        android:layout_width="150dp"
        android:layout_height="140dp"
        android:layout_marginBottom="65dp"
        android:src="@drawable/winnertrophy1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/replay_Btn_dialog_draw"
        android:layout_width="wrap_content"
        android:layout_height="43dp"
        android:layout_marginEnd="28dp"
        android:backgroundTint="@color/green2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.935" />


    <Button
        android:id="@+id/cancel_dialog_winner"
        android:layout_width="wrap_content"
        android:layout_height="45dp"
        android:layout_marginStart="28dp"
        android:backgroundTint="#CC0606"
        android:text="X"
        android:textColor="@color/white"
        android:textSize="18dp"
        android:textStyle="bold"
        app:layout_constraintBaseline_toBaselineOf="@+id/replay_Btn_dialog_draw"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/replay"
        app:layout_constraintBottom_toBottomOf="@+id/replay_Btn_dialog_draw"
        app:layout_constraintEnd_toEndOf="@+id/replay_Btn_dialog_draw"
        app:layout_constraintHorizontal_bias="0.484"
        app:layout_constraintStart_toStartOf="@+id/replay_Btn_dialog_draw"
        app:layout_constraintTop_toTopOf="@+id/replay_Btn_dialog_draw"
        app:layout_constraintVertical_bias="0.526" />

    <!--    <ImageView-->
    <!--        android:layout_width="24dp"-->
    <!--        android:layout_height="24dp"-->
    <!--        android:src="@drawable/exit"-->
    <!--        app:layout_constraintBottom_toBottomOf="@+id/cancel_dialog_winner"-->
    <!--        app:layout_constraintEnd_toEndOf="@+id/cancel_dialog_winner"-->
